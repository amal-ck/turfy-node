<section>
    <div class="container">

        <table class="table mt-3" id="productList">
            <thead>
                <tr>
                     <th scope="col">NO</th>
      <th scope="col">Name</th>
      <th scope="col">Phone No</th>
      <th scope="col">City</th>
      <th scope="col">Turf Time</th>
      <th scope="col">Image</th>
      <th>Edit</th>
      <th>Delete/th>
                </tr>
            </thead>
            <tbody>
                {{#each turfs}}
                <tr>
                    <th scope="row" class="rowNum"></th>
                    <td>{{this.Tname}}</td>
                    <td>{{this.Mobileno}}</td>
                    <td>{{this.City}}</td>
                    <td>{{this.turfTime}}</td>
                    <td><img style="width: 40px;height:50px" src="/turf-images/{{this._id}}.jpg" alt=""></td>
                    <td><a href="/owner/edit-turf/{{this._id}}" class="btn btn-primary">Edit</a></td>
                    <td><a href="/owner/delete-turf/{{this._id}}" class="btn btn-danger"
                            onclick="return confirm('Do you want delete {{this.Name}} ?')">Delete</a></td>
                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</section>


<script>
    $(document).ready(function () {
        $('#productList').DataTable();
    });
</script>